#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fpaboim/projects/appdev/fabo/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/bin/node_modules:/home/fpaboim/projects/appdev/fabo/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/node_modules:/home/fpaboim/projects/appdev/fabo/node_modules/.pnpm/js-yaml@4.1.0/node_modules:/home/fpaboim/projects/appdev/fabo/node_modules/.pnpm/node_modules:/home/fpaboim/projects/appdev/fabo/node_modules:/home/fpaboim/projects/appdev/node_modules:/home/fpaboim/projects/node_modules:/home/fpaboim/node_modules:/home/node_modules:/node_modules:/home/fpaboim/projects/appdev/fabo/.fabo/server-templates/node_modules:/home/fpaboim/projects/appdev/fabo/.fabo/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/fpaboim/projects/appdev/fabo/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/bin/node_modules:/home/fpaboim/projects/appdev/fabo/node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/node_modules:/home/fpaboim/projects/appdev/fabo/node_modules/.pnpm/js-yaml@4.1.0/node_modules:/home/fpaboim/projects/appdev/fabo/node_modules/.pnpm/node_modules:/home/fpaboim/projects/appdev/fabo/node_modules:/home/fpaboim/projects/appdev/node_modules:/home/fpaboim/projects/node_modules:/home/fpaboim/node_modules:/home/node_modules:/node_modules:/home/fpaboim/projects/appdev/fabo/.fabo/server-templates/node_modules:/home/fpaboim/projects/appdev/fabo/.fabo/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/js-yaml@4.1.0/node_modules/js-yaml/bin/js-yaml.js" "$@"
fi
