#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/@babel+node@7.14.7_@babel+core@7.14.6/node_modules/@babel/node/bin/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/@babel+node@7.14.7_@babel+core@7.14.6/node_modules/@babel/node/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/@babel+node@7.14.7_@babel+core@7.14.6/node_modules/@babel/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/@babel+node@7.14.7_@babel+core@7.14.6/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules:/home/fpaboim/projects/appdev/node_modules:/home/fpaboim/projects/node_modules:/home/fpaboim/node_modules:/home/node_modules:/node_modules:/home/fpaboim/projects/appdev/fabo_template/.fabo/server-core/node_modules:/home/fpaboim/projects/appdev/fabo_template/.fabo/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/@babel+node@7.14.7_@babel+core@7.14.6/node_modules/@babel/node/bin/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/@babel+node@7.14.7_@babel+core@7.14.6/node_modules/@babel/node/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/@babel+node@7.14.7_@babel+core@7.14.6/node_modules/@babel/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/@babel+node@7.14.7_@babel+core@7.14.6/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules:/home/fpaboim/projects/appdev/node_modules:/home/fpaboim/projects/node_modules:/home/fpaboim/node_modules:/home/node_modules:/node_modules:/home/fpaboim/projects/appdev/fabo_template/.fabo/server-core/node_modules:/home/fpaboim/projects/appdev/fabo_template/.fabo/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@babel+node@7.14.7_@babel+core@7.14.6/node_modules/@babel/node/bin/babel-node.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@babel+node@7.14.7_@babel+core@7.14.6/node_modules/@babel/node/bin/babel-node.js" "$@"
fi
