#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/dist/bin/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/dist/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/cross-env@5.2.1/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules:/home/fpaboim/projects/appdev/node_modules:/home/fpaboim/projects/node_modules:/home/fpaboim/node_modules:/home/node_modules:/node_modules:/home/fpaboim/projects/appdev/fabo_template/.fabo/server-core/node_modules:/home/fpaboim/projects/appdev/fabo_template/.fabo/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/dist/bin/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/dist/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/cross-env@5.2.1/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules/.pnpm/node_modules:/home/fpaboim/projects/appdev/fabo_template/node_modules:/home/fpaboim/projects/appdev/node_modules:/home/fpaboim/projects/node_modules:/home/fpaboim/node_modules:/home/node_modules:/node_modules:/home/fpaboim/projects/appdev/fabo_template/.fabo/server-core/node_modules:/home/fpaboim/projects/appdev/fabo_template/.fabo/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/dist/bin/cross-env-shell.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/cross-env@5.2.1/node_modules/cross-env/dist/bin/cross-env-shell.js" "$@"
fi
