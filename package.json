    "watch": [
      "packages/*",
      "./models/*",
      "./shared/*"
    ],
    "ext": ".js, .json, .yaml, .hbs"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "express-rate-limit": "^5.3.0"
