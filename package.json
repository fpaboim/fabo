{
  "name": "fabo",
  "version": "0.0.5",
  "description": "svelte express mongo (serverless) stack - pre-alpha!",
  "main": "./.fabo/index.js",
  "type": "module",
  "devDependencies": {
    "@changesets/cli": "^2.16.0",
    "concurrently": "^6.2.0",
    "wait-on": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fpaboim/fabo.git"
  },
  "bin": {
    "@fpaboim/fabo": "./.fabo/cli/bin/fabo",
    "fabo": "./.fabo/cli/bin/fabo"
  },
  "scripts": {
    "fabo": "nodemon ./index.js",
    "clean": "rm -rf node_modules server/node_modules .fabo/*/node_modules",
    "client:dev": "wait-on tcp:4000 && cd client && pnpm run dev",
    "server:dev": "cd server && pnpm run dev",
    "server:dev_prod": "cd server && pnpm run dev_prod",
    "server:deploy": "cd server && pnpm run deploy",
    "dev": "concurrently -k -n \"fabo,server,client\" -c \"magenta.bold,yellow.bold,blue.bold\" \"pnpm run fabo\" \"pnpm run server:dev\" \"pnpm run client:dev\" ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=14"
  },
  "author": "",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "server/*",
      ".fabo/*/node_modules/*",
      "docs/*"
    ],
    "watch": [
      ".fabo/*",
      "./models/*",
      "./shared/*"
    ],
    "ext": ".js, .json, .yaml, .hbs"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "express-rate-limit": "^5.3.0"
  }
}
