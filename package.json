{
  "name": "fabo",
  "version": "1.0.0",
  "description": "Serverless Express/MongoDB api generator - beta",
  "main": "./index.js",
  "type": "module",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.8",
    "@changesets/cli": "^2.16.0",
    "concurrently": "^6.2.0",
    "wait-on": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fpaboim/fabo.git"
  },
  "bin": {
    "@fpaboim/fabo": "./packages/cli/bin/fabo.js",
    "fabo": "./packages/cli/bin/fabo.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=14"
  },
  "author": "",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "server/*",
      "packages/*/node_modules/*",
      "docs/*"
