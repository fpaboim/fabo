import mongoose from 'mongoose'
import schema from './schema.js'
{{#if hasHooks}}
import hooks from './schemaHooks.js'
{{/if}}

{{#if hasHooks}}
// hooks
///////////////////////////////////////////////////////////////////////////////
for (let hook in hooks) {
  for (let hookmethod in hooks[hook]) {
    schema[hook](hookmethod, hooks[hook][hookmethod])
  }
}
{{/if}}

const {{name}} = mongoose.model('{{name}}', schema)
export default {{name}}



