    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "babel-loader": "^8.2.2",
    "concurrently": "^3.6.0",
    "faker": "^4.1.0",
    "lodash": "^4.17.21",
    "nodemon": "^1.19.4",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-offline": "^7.0.0",
