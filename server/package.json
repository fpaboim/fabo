{
  "name": "semstack_backend",
  "version": "0.0.1",
  "private": true,
  "description": "semstack_backend API server",
  "main": "server.js",
  "scripts": {
    "prestart": "mongo admin --eval 'db.shutdownServer()'; mongod --config ./db/mongodb.conf",
    "start": "cross-env NODE_ENV=development babel-node server.js --debug",
    "dev": "sls offline --stage dev --printOutput start",
    "dev_prod": "sls offline --stage prod --printOutput start",
    "deploy": "sls deploy --stage prod --verbose",
    "poststart": "mongo admin --eval 'db.shutdownServer()'"
  },
  "imports": {
    "#shared/*": "../shared/*"
  },
  "keywords": [],
  "author": "Francisco Aboim",
  "license": "CC",
  "dependencies": {
    "@middy/core": "^2.4.1",
    "@middy/http-cors": "^2.4.1",
    "@vendia/serverless-express": "^4.3.9",
    "shared": "workspace:0.0.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^2.4.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dayjs": "^1.10.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "helmet": "^3.15.0",
    "helmet-crossdomain": "^0.3.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.4.0",
    "md5": "^2.2.1",
    "mongodb-client-encryption": "^1.2.5",
    "mongoose": "^5.4.10",
    "mongoose-validator": "^2.1.0",
    "netjet": "^1.3.0",
    "npm": "^7.17.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "saslprep": "^1.0.3",
    "serverless-api-gateway-throttling": "^1.1.0",
